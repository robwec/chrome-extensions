{
  "manifest_version": 2,

  "name": "Duplicate Tabs Shortcut",
  "description": "Adds a command to duplicate active tab. Map the shortcut at the bottom of the extensions page chrome://extensions",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
  ],
  "commands": {
    "dupe tab": {
      "suggested_key": {
        "default": "Alt+H"
      },
      "description": "Duplicates the active tab."
    }
  }
}